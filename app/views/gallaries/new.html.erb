<div class="row">
  <div class="col-md-12">
    <h1>Create Gallary</h1>
    <%= form_for @gallary do |f| %>
        <div class="form-group">
          <%= f.label :image %>
          <%= f.file_field :image, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.label :alt %>
          <%= f.text_field :alt, class: "form-control", rows: 4 %>
        </div>
        <%= f.submit "Create Gallary", class: "btn btn-success" %>
    <% end %>
  </div>
</div>